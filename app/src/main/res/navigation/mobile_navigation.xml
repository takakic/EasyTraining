<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="jp.takakic.easytraining.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_eating"
        android:name="jp.takakic.easytraining.ui.eating.EatingFragment"
        android:label="@string/title_eating"
        tools:layout="@layout/fragment_eating" />

    <fragment
        android:id="@+id/navigation_exercise"
        android:name="jp.takakic.easytraining.ui.exercise.ExerciseFragment"
        android:label="@string/title_exercise"
        tools:layout="@layout/fragment_exercise" />

    <fragment
        android:id="@+id/navigation_body"
        android:name="jp.takakic.easytraining.ui.body.BodyFragment"
        android:label="@string/title_body"
        tools:layout="@layout/fragment_body" />
</navigation>